<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>net.codesapien</groupId>
  <artifactId>liber8apps</artifactId>
  <version>1.0-SNAPSHOT</version>
  <name>liber8apps</name>
  <description>Project base for Vaadin Flow and Spring Boot</description>
  <url>https://github.com/viritin/viritin-sb/liber8apps</url>
  <organization>
    <name>Viritin</name>
  </organization>
  <licenses>
    <license>
      <name>Apache 2</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <developers>
    <developer>
      <name>Matti Tahvonen</name>
    </developer>
  </developers>
  <scm>
    <connection>scm:git:git://github.com/viritin/viritin-sb.git/liber8apps</connection>
    <developerConnection>scm:git:ssh://git@github.com:/viritin/viritin-sb.git/liber8apps</developerConnection>
    <tag>viritin-project-parent-0.0.11</tag>
    <url>https://github.com/viritin/viritin-sb/liber8apps</url>
  </scm>
  <issueManagement>
    <system>GitHub</system>
    <url>https://github.com/viritin/viritin-sb/issues</url>
  </issueManagement>
  <dependencies>
    <dependency>
      <groupId>org.springframework.boot</groupId>
      <artifactId>spring-boot-devtools</artifactId>
      <version>3.4.0</version>
      <scope>compile</scope>
      <optional>true</optional>
    </dependency>
    <dependency>
      <groupId>org.springframework.ai</groupId>
      <artifactId>spring-ai-starter-model-ollama</artifactId>
      <version>1.0.0</version>
      <scope>compile</scope>
    </dependency>
  </dependencies>
  <repositories>
    <repository>
      <releases>
        <enabled>true</enabled>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>spring-ai</id>
      <name>Spring AI Repository</name>
      <url>https://repo.spring.io/spring-ai</url>
    </repository>
    <repository>
      <releases>
        <enabled>false</enabled>
      </releases>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
      <id>spring-milestones</id>
      <name>Spring Milestones</name>
      <url>https://repo.spring.io/milestone</url>
    </repository>
    <repository>
      <releases>
        <enabled>false</enabled>
      </releases>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
      <id>spring-snapshots</id>
      <name>Spring Snapshots</name>
      <url>https://repo.spring.io/snapshot</url>
    </repository>
    <repository>
      <releases>
        <enabled>true</enabled>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>vaadin-addons</id>
      <name>Vaadin Addons</name>
      <url>https://maven.vaadin.com/vaadin-addons</url>
    </repository>
    <repository>
      <releases>
        <enabled>false</enabled>
      </releases>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
      <id>vaadin-prereleases</id>
      <name>Vaadin Pre-Releases</name>
      <url>https://maven.vaadin.com/vaadin-prereleases</url>
    </repository>
  </repositories>
</project>
